{
  "$schema": "vscode://schemas/color-theme",
  "name": "Andiors Nord Pastel",
  "type": "dark",
  "colors": {
    "activityBar.background": "#3B4252",
    "activityBar.foreground": "#D8DEE9",
    "activityBarBadge.background": "#B0C9D8",
    "activityBarBadge.foreground": "#2E3440",
    "sideBar.background": "#2E3440",
    "sideBar.foreground": "#D8DEE9",
    "statusBar.background": "#3B4252",
    "statusBar.foreground": "#D8DEE9",
    "editor.background": "#2E3440",
    "editor.foreground": "#D8DEE9",
    "editorCursor.foreground": "#D8DEE9",
    "editorLineNumber.foreground": "#8F9BB3",
    "editorLineNumber.activeForeground": "#D8DEE9",
    "tab.activeBackground": "#3B4252",
    "tab.inactiveBackground": "#2E3440",
    "tab.activeForeground": "#D8DEE9",
    "tab.inactiveForeground": "#8F9BB3",
    "terminal.background": "#2E3440",
    "terminal.foreground": "#D8DEE9",

    "window.activeBorder": "#B0C9D8",
    "titleBar.activeBackground": "#3B4252",
    "titleBar.activeForeground": "#D8DEE9",
    "titleBar.inactiveBackground": "#434C5E",
    "titleBar.inactiveForeground": "#8F9BB3",
    "menu.background": "#3B4252",
    "menu.foreground": "#D8DEE9",
    "menu.selectionBackground": "#B0C9D8",
    "menu.selectionForeground": "#2E3440",
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#A3B1C2"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#D6999F"  // pastel red
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#8FAEC9"  // pastel blue
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#C1BFA7"  // pastel beige
      }
    },
    {
      "scope": ["variable", "entity.name.variable"],
      "settings": {
        "foreground": "#8FBFB2"  // pastel teal green
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#D99694"  // pastel warm red
      }
    },
    {
      "scope": [
        "storage.type.class",
        "entity.name.type.class",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#B0C4A7", // pastel green
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#B0C4A7"  // pastel green
      }
    },
    {
      "scope": ["support.function", "support.constant"],
      "settings": {
        "foreground": "#8FAEC9"  // pastel blue
      }
    },
    {
      "scope": ["entity.name.type.enum", "entity.name.enum"],
      "settings": {
        "foreground": "#D6999F",
        "fontStyle": "bold"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": {
      "foreground": "#8FBFB2"
    },
    "parameter": {
      "foreground": "#C1BFA7"
    },
    "function": {
      "foreground": "#8FAEC9",
      "fontStyle": "bold"
    },
    "method": {
      "foreground": "#8FAEC9",
      "fontStyle": "bold"
    },
    "property": {
      "foreground": "#D8DEE9"
    },
    "class": {
      "foreground": "#B0C4A7",
      "fontStyle": "bold"
    },
    "enum": {
      "foreground": "#D6999F",
      "fontStyle": "bold"
    },
    "interface": {
      "foreground": "#B0C4A7"
    },
    "type": {
      "foreground": "#AFC5B8"
    },
    "constant": {
      "foreground": "#D99694"
    },
    "keyword": {
      "foreground": "#D99694"
    },
    "string": {
      "foreground": "#B0C4A7"
    },
    "number": {
      "foreground": "#D6999F"
    },
    "comment": {
      "foreground": "#A3B1C2"
    }
  }
}
